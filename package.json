{
  "name": "template4website",
  "version": "1.0.0",
  "description": "Website - Project template.",
  "type": "module",
  "keywords": [
    "sass",
    "scss",
    "css"
  ],
  "license": "MIT",
  "author": "Nkwati Divine <drnkwati+github@gmail.com> (https://github.com/drnkwati)",
  "homepage": "https://github.com/dnkwati/template4website#readme",
  "bugs": {
    "url": "https://github.com/dnkwati/template4website/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnkwati/template4website.git"
  },
  "scripts": {
    "dev": "rm -rf out && bash src/bash/exe.sh dev.scss & bash src/bash/exe.sh dev.mjs & bash src/bash/exe.sh dev.js & bash src/bash/exe.sh dev.assets & bash src/bash/exe.sh dev.watch",
    "dev.assets": "./node_modules/.bin/copy-and-watch",
    "dev.js": "./node_modules/.bin/esbuild",
    "dev.mjs": "./node_modules/.bin/tsc",
    "dev.scss": "./node_modules/.bin/sass",
    "dev.watch": "./node_modules/.bin/browser-sync",
    "dist": "rm -rf dist && (bash src/bash/exe.sh dist.scss && bash src/bash/exe.sh dist.css) && (bash src/bash/exe.sh dist.mjs && bash src/bash/exe.sh dist.js) && bash src/bash/exe.sh dist.assets",
    "dist.assets": "./node_modules/.bin/copy-and-watch",
    "dist.css": "./node_modules/.bin/esbuild",
    "dist.js": "./node_modules/.bin/esbuild",
    "dist.mjs": "./node_modules/.bin/tsc",
    "dist.scss": "./node_modules/.bin/sass",
    "preinstall": "node src/exe/preinstall.js"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "copy-and-watch": "latest",
    "esbuild": "latest",
    "sass": "latest",
    "typescript": "latest"
  }
}
